      <add key="escribeArchivoLOG.LogPath" value="D:\Procesos\TestMonitorMQTKTNet\Monitores\log\" />
      <!--<add key="escribeArchivoLOG.LogPath" value="C:\tmp\log\" />-->
      <add key="escribeArchivoLOG.LogFile" value="LogMonitorMQTKT" />
